package fr.polytech.fsback.dto;

import lombok.AllArgsConstructor;
import lombok.Data;

@Data
@AllArgsConstructor
public class ErrorMessagesDto {
	public int code;
	public String message;
	
	public ErrorMessagesDto(int code, String message) {
		super();
		this.code = code;
		this.message = message;
	}
	
	
	
}
